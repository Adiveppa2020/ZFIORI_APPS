<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<SelectDialog id="idBlockCodeDialog" title="{i18n>blockCodeList}"	items="{/zblock_code_DelSet}"
		search=".onValueHelpSearch"	confirm=".onValueHelpClose($event, '/filterValues/LIFSP')" 
		cancel=".onValueHelpClose($event, '/filterValues/LIFSP')"
		customData:FilterProperty="{prop1: 'LIFSP', prop2: 'Vtext'}"
		customData:inputId="idBlockCodeInput">
		<StandardListItem title="{LIFSP}" description="{Vtext}"/>
	</SelectDialog>
</core:FragmentDefinition>
